{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions":[
        {
            "name": "client",
            "image": "missym2/multi-client",
            "hostname":"client",
            "essential": false,
            "memory": 128
        },
        {
            "name":"server",
            "image": "missym2/multi-server",
            "hostname": "api",
            "essential":false,
            "memory": 128
        },
        {
            "name": "worker",
            "image":"missym2/multi-worker",
            "hostname":"worker",
            "essential":false,
            "memory": 128
        },
        {
            "name":"nginx",
            "image": "missym2/multi-nginx",
            "hostname":"nginx",
            "essential":true,
            "portMappings":[
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
                
            ],
            "links":["client", "server"],
            "memory": 128
        }
    ]
}